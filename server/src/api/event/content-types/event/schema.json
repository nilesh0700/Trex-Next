{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": "Event content type for managing events"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "event_date": {
      "type": "datetime"
    },
    "location": {
      "type": "string"
    },
    "participants_count": {
      "type": "string"
    },
    "registration_link": {
      "type": "string"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "overlay_text": {
      "type": "string"
    },
    "contact_email": {
      "type": "email"
    },
    "contact_time": {
      "type": "string"
    },
    "contact_location": {
      "type": "string"
    },
    "event_heading": {
      "type": "string"
    },
    "event_subheading": {
      "type": "text"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-category.event-category",
      "inversedBy": "events"
    },
    "organizer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organizer.organizer",
      "inversedBy": "events"
    },
    "target_cities": {
      "type": "component",
      "repeatable": true,
      "component": "shared.target-city"
    },
    "city_statistics": {
      "type": "component",
      "repeatable": true,
      "component": "shared.city-statistic"
    },
    "target_markets_title": {
      "type": "string",
      "default": "Target Markets Across India"
    },
    "target_markets_description": {
      "type": "text"
    },
    "pricing_packages": {
      "type": "component",
      "repeatable": true,
      "component": "shared.pricing-package"
    },
    "pricing_section_title": {
      "type": "string",
      "default": "Space And Proposal"
    },
    "pricing_section_description": {
      "type": "text",
      "default": "Choose The Perfect Package For Your Networking Goals"
    },
    "event_flow_items": {
      "type": "component",
      "repeatable": true,
      "component": "shared.event-flow-item"
    },
    "event_flow_title": {
      "type": "string",
      "default": "Event Flow"
    },
    "event_flow_description": {
      "type": "text",
      "default": "Two Days of Intensive Networking and Business Development"
    }
  }
} 